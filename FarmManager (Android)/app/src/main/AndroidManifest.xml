<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rasicmihailo.farmmanager">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/farma"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VlasnikActivity"
            android:label="@string/title_activity_vlasnik"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RegistracijaVlasnikActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Registracija2Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegistracijaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RadnikActivity"
            android:label="@string/title_activity_radnik"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NeradnikActivity"
            android:label="@string/title_activity_neradnik"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LoginActivity"></activity>
    </application>

</manifest>